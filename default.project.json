{
  "name": "Nucleard",
  "globIgnorePaths": ["**/*.spec.luau", "**/*.test.luau"],
  "tree": {
    "$className": "DataModel",

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "Gravity": 196.2
      },
      "Baseplate": {
        "$className": "SpawnLocation",
        "$properties": {
          "Size": [512, 1, 512],
          "Position": [0, -0.5, 0],
          "Anchored": true,
          "Color": [240, 180, 160],
          "Material": "SmoothPlastic",
          "TopSurface": "Smooth",
          "BottomSurface": "Smooth",
          "Transparency": 0,
          "CanCollide": true,
          "Enabled": true,
          "Neutral": true,
          "Duration": 0
        }
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/shared",
      "Packages": {
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "$path": "src/server",
      "Packages": {
        "$path": "ServerPackages"
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "$ignoreUnknownInstances": true,
      "$path": "src/ServerStorage"
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$ignoreUnknownInstances": true,
        "$path": "src/client"
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "$ignoreUnknownInstances": true,
      "$path": "src/StarterGui"
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Brightness": 2,
        "Technology": "ShadowMap",
        "ClockTime": 14.5,
        "Ambient": [40, 30, 50]
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
